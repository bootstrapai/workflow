{
    "name": "@bootstrapai/workflow",
    "version": "0.1.0-development",
    "description": "",
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "test": "jest",
        "build": "npm run build:cjs && npm run build:esm",
        "build:cjs": "tsc --emitDeclarationOnly && BABEL_ENV=commonjs babel src --out-dir dist/cjs --extensions '.ts,.tsx' --source-maps inline",
        "build:esm": "tsc --emitDeclarationOnly && BABEL_ENV=esm babel src --out-dir dist/esm --extensions '.ts,.tsx' --source-maps inline",
        "semantic-release": "semantic-release"
    },
    "author": "",
    "license": "MIT",
    "dependencies": {
        "dotenv": "^16.3.1",
        "inquirer": "^9.2.7",
        "openai-queue": "^1.2.2",
        "readline": "^1.3.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.22.5",
        "@babel/preset-env": "^7.22.5",
        "@babel/preset-typescript": "^7.22.5",
        "@types/dotenv": "^8.2.0",
        "@types/inquirer": "^9.0.3",
        "@types/jest": "^29.5.2",
        "@types/mocha": "^10.0.1",
        "chai": "^4.3.7",
        "jest": "^29.5.0",
        "semantic-release": "^21.0.5",
        "sinon": "^15.2.0",
        "ts-jest": "^29.1.0",
        "ts-mocha": "^10.0.0",
        "ts-node": "^10.9.1",
        "tsimportlib": "^0.0.5",
        "typescript": "^5.1.3"
    },
    "exports": {
        ".": {
            "import": "./dist/esm/index.js",
            "require": "./dist/cjs/index.js"
        }
    },
    "release": {
        "branches": [
            "main",
            "next"
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/bootstrapai/workflow.git"
    }
}
